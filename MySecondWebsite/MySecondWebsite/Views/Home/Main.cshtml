@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Generic StoreFront</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <img id="background" src="~/Content/bigfootbackground.gif" />
    <div class="container mainpage">
        <div class="row">
            <h1 class="">GENERIC STORE INC.</h1>
        </div>
        <div class="row">
            <p class="">Hey bud, go ahead and click that little ditty there to join the club</p>
        </div>
        <div class="row">
            <button type="button" class="btn" onclick="location.href = '@Url.Action("Register", "Home")'">Register</button>
            @*@(Html.ActionLink("Register", "Register", null, new { @class = "btn" }))*@
        </div>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</body>
</html>

